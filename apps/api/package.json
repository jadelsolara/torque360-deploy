    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^7.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.17.5",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.0"
  }
