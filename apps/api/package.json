{
  "name": "@torque360/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "stress": "bash test/k6/run-stress.sh",
    "stress:smoke": "bash test/k6/run-stress.sh smoke",
    "stress:load": "bash test/k6/run-stress.sh load",
    "stress:stress": "bash test/k6/run-stress.sh stress",
    "stress:spike": "bash test/k6/run-stress.sh spike",
    "stress:soak": "bash test/k6/run-stress.sh soak",
    "stress:auth": "bash test/k6/run-stress.sh auth",
    "stress:wo": "bash test/k6/run-stress.sh work-orders",
    "stress:inventory": "bash test/k6/run-stress.sh inventory",
    "stress:dashboard": "bash test/k6/run-stress.sh dashboard",
    "stress:payroll": "bash test/k6/run-stress.sh payroll",
    "stress:suppliers": "bash test/k6/run-stress.sh suppliers",
    "stress:lifecycle": "bash test/k6/run-stress.sh full-lifecycle",
    "migration:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/database/data-source.ts",
    "migration:run": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "migration:revert": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.992.0",
