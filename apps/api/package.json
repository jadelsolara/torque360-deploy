    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.0",
    "@sentry/nestjs": "^10.39.0",
    "@sentry/node": "^10.39.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.3.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.0",
    "meilisearch": "^0.55.0",
    "mercadopago": "^2.12.0",
    "nestjs-pino": "^4.6.0",
    "nodemailer": "^8.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.13.0",
