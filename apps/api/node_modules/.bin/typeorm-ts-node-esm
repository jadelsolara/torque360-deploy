#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jose-antonio/Desktop/Proyectos/Torque360/node_modules/.pnpm/typeorm@0.3.28_ioredis@5.9.3_pg@8.18.0/node_modules/typeorm/node_modules:/home/jose-antonio/Desktop/Proyectos/Torque360/node_modules/.pnpm/typeorm@0.3.28_ioredis@5.9.3_pg@8.18.0/node_modules:/home/jose-antonio/Desktop/Proyectos/Torque360/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jose-antonio/Desktop/Proyectos/Torque360/node_modules/.pnpm/typeorm@0.3.28_ioredis@5.9.3_pg@8.18.0/node_modules/typeorm/node_modules:/home/jose-antonio/Desktop/Proyectos/Torque360/node_modules/.pnpm/typeorm@0.3.28_ioredis@5.9.3_pg@8.18.0/node_modules:/home/jose-antonio/Desktop/Proyectos/Torque360/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
